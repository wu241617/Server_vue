{
  "name": "servervueantd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "ant-design-vue": "^1.4.3",
    "rxjs": "^6.5.4",
    "viser-vue": "^2.4.8",
    "vue": "^2.6.10",
    "vue-i18n": "^8.15.0",
    "vue-router": "^3.0.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@antv/data-set": "^0.10.2",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "antd-theme-webpack-plugin": "^1.3.1",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "core-js": "^3.1.2",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^2.7.2",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {
      "no-console": 1,
      "indent": [
        2,
        2
      ],
      "eqeqeq": 2,
      "semi": [
        2,
        "never"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ]
}
